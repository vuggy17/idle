{
  "extends": [
    "../../.eslintrc.json"
  ],
  "parserOptions": {
    "project": [
      "apps/idle.api/tsconfig.eslint.json"
    ]
  },
  "ignorePatterns": [
    "!**/*", "jest.config.ts"
  ],
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "rules": {
        "import/prefer-default-export": [
          "off"
        ],
        "no-underscore-dangle": [
          "error",
          {
            "allowAfterThis": true
          }
        ]
      }
    }
  ]
}
